    "@remix-run/react": "^1.1.1",
    "@remix-run/serve": "^1.1.1",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "phosphor-react": "^1.4.1",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.33.0",
    "react-table": "^7.8.0",
    "remix": "^1.1.1",
    "tailwindcss": "^3.0.12",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.1",
